{
  "name": "geostyler",
  "version": "0.18.0",
  "description": "Framework for styling geodata",
  "main": "dist/index.js",
  "files": [
    "dist",
    "browser"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terrestris/geostyler.git"
  },
  "keywords": [
    "geo",
    "styler",
    "geostyler"
  ],
  "author": "",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/terrestris/geostyler/issues"
  },
  "homepage": ".",
  "scripts": {
    "build": "npm run build:dist && npm run build:app && npm run build:browser && npm run build:styleguide",
    "build:app": "node scripts/build.js",
    "build:dist": "tsc -p ./ && copyfiles \"./src/**/*.css\" dist --up 1",
    "build:styleguide": "styleguidist build",
    "build:browser": "webpack --config browser-build.config.js",
    "lint": "tslint --project tsconfig.json --config tslint.json && tsc --noEmit --project tsconfig.json",
    "lint:prod": "tslint --project tsconfig.json --config tslint.prod.json && tsc --noEmit --project tsconfig.json",
    "pretest": "npm run lint:prod",
    "prebuild": "rimraf dist/** && rimraf build/**",
    "prepublishOnly": "npm run build",
    "release": "np --no-yarn && git push https://github.com/terrestris/geostyler.git master --tags",
    "styleguide": "styleguidist server",
    "start": "node scripts/start.js",
    "start:styleguide": "styleguidist server",
    "test": "CI=true node scripts/test.js --env=jsdom --runInBand --transformIgnorePatterns \"<rootDir>/node_modules/(?!ol|antd|codemirror)\"",
    "test:watch": "node scripts/test.js --env=jsdom --transformIgnorePatterns \"<rootDir>/node_modules/(?!ol|antd|codemirror)\""
  },
  "dependencies": {
    "antd": "3.10.4",
    "autoprefixer": "7.1.6",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.2",
    "blob": "0.0.5",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "codemirror": "5.41.0",
    "color": "3.1.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "file-saver": "1.3.8",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "geostyler-data": "0.5.2",
    "geostyler-geojson-parser": "0.4.5",
    "geostyler-openlayers-parser": "0.18.2",
    "geostyler-sld-parser": "0.17.2",
    "geostyler-style": "0.14.2",
    "geostyler-wfs-parser": "0.7.3",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "object-assign": "4.1.1",
    "ol": "4.6.5",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.6.0",
    "react-codemirror2": "5.1.0",
    "react-color": "2.14.1",
    "react-dev-utils": "^5.0.2",
    "react-dom": "16.6.0",
    "react-rnd": "9.0.2",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "0.6.2",
    "url-search-params": "1.1.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/codemirror": "0.0.66",
    "@types/color": "3.0.0",
    "@babel/core": "7.1.5",
    "@types/enzyme": "3.1.15",
    "@types/file-saver": "2.0.0",
    "@types/geojson": "7946.0.4",
    "@types/jest": "23.3.9",
    "@types/lodash": "4.14.118",
    "@types/node": "10.12.3",
    "@types/ol": "4.6.2",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.5",
    "@babel/preset-stage-0": "7.0.0",
    "@types/prop-types": "15.5.6",
    "@types/react": "16.4.18",
    "@types/react-color": "2.13.6",
    "@types/react-dom": "16.0.9",
    "awesome-typescript-loader": "4.0.1",
    "babel-loader": "8.0.4",
    "canvas-prebuilt": "1.6.11",
    "copyfiles": "2.1.0",
    "coveralls": "3.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "extract-text-webpack-plugin": "3.0.2",
    "np": "3.0.4",
    "react-docgen-typescript": "1.12.0",
    "react-styleguidist": "7.3.11",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.6",
    "typescript": "3.1.4",
    "uglifyjs-webpack-plugin": "1.1.2",
    "webpack": "3.12.0",
    "webpack-cli": "3.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/Component/**/*.{tsx,jsx}",
      "src/DataProvider/**/*.{tsx,jsx}",
      "src/Util/**/*.{tsx,jsx}",
      "src/app/**/*.{tsx,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.ts",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "/home/jsuleiman/Projekte/geostyler/tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
